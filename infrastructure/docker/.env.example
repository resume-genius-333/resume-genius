# Copy this file to .env and update the values

# PostgreSQL Configuration
POSTGRES_PASSWORD=your-secure-postgres-password

# ClickHouse Configuration
CLICKHOUSE_USER=clickhouse
CLICKHOUSE_PASSWORD=your-secure-clickhouse-password

# MinIO Configuration
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=your-secure-minio-password
MINIO_DOMAIN=localhost

# Redis Configuration
REDIS_AUTH=your-secure-redis-password

# Langfuse Configuration
SALT=your-32-character-salt-value
# Generate ENCRYPTION_KEY: openssl rand -hex 32
ENCRYPTION_KEY=your-64-character-hex-encryption-key
# Generate NEXTAUTH_SECRET: openssl rand -base64 32
NEXTAUTH_SECRET=your-nextauth-secret-key
NEXTAUTH_URL=http://localhost:3000

# Langfuse Initial User
LANGFUSE_INIT_USER_EMAIL=admin@example.com
LANGFUSE_INIT_USER_PASSWORD=your-secure-admin-password
LANGFUSE_INIT_USER_NAME=Admin User
LANGFUSE_INIT_ORG_ID=my-organization
LANGFUSE_INIT_ORG_NAME=My Organization
LANGFUSE_INIT_PROJECT_ID=my-project
LANGFUSE_INIT_PROJECT_NAME=My Project
LANGFUSE_INIT_PROJECT_PUBLIC_KEY=pk-lf-your-public-key
LANGFUSE_INIT_PROJECT_SECRET_KEY=sk-lf-your-secret-key

# LiteLLM Configuration
LITELLM_MASTER_KEY=sk-your-secure-litellm-master-key

# LLM Provider API Keys (for LiteLLM)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
GROQ_API_KEY=gsk_your-groq-api-key
DEEPSEEK_API_KEY=sk-your-deepseek-api-key
# AZURE_API_KEY=your-azure-api-key

# Langfuse Integration Keys for LiteLLM
# These should match the LANGFUSE_INIT_PROJECT keys above
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key

# Optional: Enable/Disable Features
TELEMETRY_ENABLED=false
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true